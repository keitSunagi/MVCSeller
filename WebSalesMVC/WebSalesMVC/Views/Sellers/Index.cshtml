@model IEnumerable<WebSalesMVC.Models.Seller>
@*
    
*@
@{
    ViewData["Title"] = "Sellers";
}
<h2>@ViewData["Title"]</h2>
<a asp-action="Create" class="btn bg-gradient">Create New Seller</a>
<br />
<br />
<table class="table table-striped table-hover">
    <thead>
        <tr class="bg-gradient">
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BirthDate)
            </th>
            <th>
                <central>@Html.DisplayNameFor(model => model.BaseSalary)</central>
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name);
                </td>
                  <td>
                    @Html.DisplayFor(modelItem => item.Email);
                </td>
                  <td>
                    @Html.DisplayFor(modelItem => item.BirthDate);
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BaseSalary);
                </td>
            </tr>
        }
    </tbody>
</table>

